<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="Inventario"/>
  <table-filter match-name="Producto"/>
  <table-filter match-name="Mes"/>
  <table-filter match-name="DatosMes"/>
  <table-filter match-name="Jornada"/>
  <table-filter match-name="DatosGenerales"/>
</hibernate-reverse-engineering>
